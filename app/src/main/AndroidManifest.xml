        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Msaada.NoActionBar">
        <activity android:name=".forms.feedbackActivity"></activity>
