        <activity android:name=".contributor.addcontributionActivity" />
        <activity android:name=".admin.AdminActivity" />
        <activity android:name=".contributionsActivity" />
        <activity android:name=".forms.registerActivity" />
        <activity android:name=".forms.loginActivity" />
        <activity android:name=".splashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>